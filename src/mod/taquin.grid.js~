***************************************/

var Grid = function(dx, dy, dz) {
    this._cubes = [];
    for (var loop = 0 ; loop < dx*dy*dz ; loop++) {
        this._cubes.push(null);
    }
    this._dx = dx;
    this._dy = dy;
    this._dz = dz;
};


/**
 * @return void
 */
Grid.prototype.cube = function(x, y, z, cube) {
    var index = Math.floor(x + y * this._dx + z * this._dx * this._dy);
    
};
